<h5>Pre√ßos</h5>

<%= render 'form' %>

<br/>

<table>
  <thead>
    <tr>
      <th>Gasoline</th>
      <th>Amount</th>
      <th>Supplier</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.gasoline %></td>
        <td><%= price.amount %></td>
        <td><%= price.supplier.name %></td>
        <td><%= link_to 'Edit', edit_price_path(price) %></td>
        <td><%= link_to 'Destroy', price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>