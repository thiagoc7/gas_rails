<div class="section">
  <%= form_tag '', method: :get do %>
    <div class="row">
      <div class="input-field col s4">
        <%= date_field_tag :begin_date, nil, class: 'datepicker', placeholder: 'data inicial' %>
      </div>
      <div class="input-field col s4">
        <%= date_field_tag :end_date, nil, class: 'datepicker', placeholder: 'data final'  %>
      </div>
      <div class="input-field col s4">
        <%= submit_tag 'Filter', class: 'btn' %>
      </div>
    </div>
  <% end %>
</div>